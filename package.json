{
  "name": "stuga",
  "version": "0.3.23",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "cap:sync": "cap sync",
    "ios": "npm run build && cap sync ios && cap open ios",
    "ios:dev": "npm run build && cap sync ios && cap run ios",
    "android": "npm run build && cap sync android && cap open android",
    "android:dev": "npm run build && cap sync android && cap run android",
    "android:bundle": "npm run build && cap sync android && cd android && ./gradlew bundleRelease",
    "release": "./scripts/release.sh",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major"
  },
  "dependencies": {
    "@capacitor-firebase/analytics": "^8.0.1",
    "@capacitor-firebase/crashlytics": "^8.0.1",
    "@capacitor-firebase/performance": "^8.0.1",
    "@capacitor/android": "^8.0.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capgo/inappbrowser": "^8.0.6",
    "@iconify/react": "^6.0.2",
    "capacitor-secure-storage-plugin": "^0.13.0",
    "clsx": "^2.1.1",
    "firebase": "^12.8.0",
    "framer-motion": "^11.12.0",
    "lucide-react": "^0.460.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.0",
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.8.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vitest": "^3.0.0"
  }
}
